$('detail_table').html('<%= escape_javascript(render partial:'detail_table', locals: {data_for_test_reports: @data_for_test_reports, select: @select} )%>');

var element = document.getElementsByName("check_status");
Object.keys(element).forEach((select) => {
    var checked = element[select].value;
    if ( checked == "1" ) {
    element[select].className="select_1";
    } else if ( checked == "2" ) {
    element[select].className="select_2";
    } else if ( checked == "3" ) {
    element[select].className="select_3";
    } else if ( checked == "4" ) {
    element[select].className="select_4";
    }
});